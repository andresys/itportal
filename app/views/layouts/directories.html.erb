<%= page_title t '.title' do %>
  <%= yield :title %>
<% end %>

<%= yield(:filter) %>

<% url = lambda {|item| [:directories, item.to_sym]} %>
<% active = lambda {|item| controller_name == item} %>
<%= render "shared/nav_tabs", items: ['organizations', 'employees', 'mols', 'locations', 'asset_types'], url: url, active: active %>

<%= yield %>

<% parent_layout :application %>